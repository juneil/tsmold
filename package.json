{
    "name": "tsmold",
    "version": "1.3.0",
    "description": "TS Molder, tranform class to json-schema",
    "main": "index.js",
    "scripts": {
        "test": "jest",
        "build": "tsc --build tsconfig.build.json && cp package.json build/ && cp README.md build/",
        "clean": "rm -r build/"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/juneil/tsmold.git"
    },
    "author": "Juneil",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/juneil/tsmold/issues"
    },
    "homepage": "https://github.com/juneil/tsmold#readme",
    "jest": {
        "coverageDirectory": "../coverage",
        "testEnvironment": "node",
        "rootDir": "./",
        "roots": [
            "./test"
        ],
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js"
        ],
        "transform": {
            "^.+\\.(ts|tsx)$": "ts-jest"
        },
        "testMatch": [
            "**/*.spec.(ts|js)"
        ],
        "globals": {
            "ts-jest": {
                "tsConfig": "tsconfig.json"
            }
        }
    },
    "devDependencies": {
        "@types/ajv": "^1.0.0",
        "@types/jest": "^25.1.5",
        "@types/node": "^13.11.0",
        "@typescript-eslint/eslint-plugin": "^2.26.0",
        "@typescript-eslint/parser": "^2.26.0",
        "eslint": "^6.8.0",
        "jest": "^25.2.7",
        "ts-jest": "^25.3.1",
        "ts-node": "^8.8.1",
        "typescript": "^3.8.3"
    },
    "dependencies": {
        "ajv": "^6.12.0",
        "reflect-metadata": "^0.1.13"
    }
}
